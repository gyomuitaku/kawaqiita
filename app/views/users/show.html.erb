  <script>
    // $('nav').removeClass('fixed-top');
    $('nav').addClass('my-bg-dark');
    $('ul').removeClass('my-d-none');
    for (let i = 1; i <= 3; i++) {
      document.getElementById('tab' + i + '-' + '1').addEventListener("click", {
        handleEvent: moveToFirst,
        name: i
      });
      document.getElementById('tab' + i + '-' + '2').addEventListener("click", {
        handleEvent: moveToSecond,
        name: i
      });
      document.getElementById('tab' + i + '-' + '3').addEventListener("click", {
        handleEvent: moveToThird,
        name: i
      });
      document.getElementById('tab' + i + '-' + '4').addEventListener("click", {
        handleEvent: moveToFour,
        name: i
      });
    }

    function moveToFirst() {
      $("#slide" + this.name).attr('class', 'move-to-first' + this.name);
      $(".tab").attr('class', 'tab');
      $("#tab" + this.name + "-1").attr('class', "tab selected");
    }

    function moveToSecond() {
      $("#slide" + this.name).attr('class', 'move-to-second' + this.name);
      $(".tab").attr('class', 'tab');
      $("#tab" + this.name + "-2").attr('class', 'selected tab');
    }

    function moveToThird() {
      $("#slide" + this.name).attr('class', 'move-to-third' + this.name);
      $(".tab").attr('class', 'tab');
      $("#tab" + this.name + "-3").attr('class', 'selected tab');
    }

    function moveToFour() {
      $("#slide" + this.name).attr('class', 'move-to-four' + this.name);
      $(".tab").attr('class', 'tab');
      $("#tab" + this.name + "-4").attr('class', 'selected tab');
    }
  </script>
